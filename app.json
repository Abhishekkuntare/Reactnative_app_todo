{
  "expo": {
    "name": "animated-todo",
    "slug": "animated-todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-xxxxxx"
      },
      "bundleIdentifier": "com.abhishekkuntare.animatedtodo"
    },
    "android": {
      "package": "com.abhishekkuntare.animatedtodo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6f9a6674-0c27-4c8d-b2b0-787ad908fffb"
      }
    }
  }
}
